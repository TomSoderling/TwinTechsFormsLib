<?xml version="1.0" encoding="UTF-8"?>

<ContentPage
	xmlns:cells="clr-namespace:TwinTechs.Example.FastCells.Cells;assembly=TwinTechsFormsExample"
	xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="TwinTechs.Example.FastCells.FastCellSamplePage"
	Title="FastCell">

	<ContentPage.Content>

		<ListView
			x:Name="MediaItemsListView"
			BackgroundColor="Transparent"
			ItemSelected="OnItemSelected"
			SeparatorVisibility="None"
			RowHeight="300">

			<ListView.ItemTemplate>

				<DataTemplate>

					<cells:SimpleFastCell />

				</DataTemplate>

			</ListView.ItemTemplate>

		</ListView>

	</ContentPage.Content>

</ContentPage>
